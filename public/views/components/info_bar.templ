package components

templ Infobar() {
	<div class="stats stats-vertical lg:stats-horizontal shadow" id="info_bar" hx-swap-oob="true">
		<div class="stat">
			<div class="stat-title">SD CARD</div>
			<div class="stat-value text-2xl">86GB/116GB</div>
		</div>
		<div class="stat">
			<div class="stat-title">Toshiba</div>
			<div class="stat-value text-2xl">634GB/953GB</div>
		</div>
		<div class="stat">
			<div class="stat-title">Ram Usage</div>
			<div class="stat-value text-2xl">
				953mb/3945mb
				(29.2%)
