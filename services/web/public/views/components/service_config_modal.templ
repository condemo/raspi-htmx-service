package components

templ ServiceConfigModal() {
	<dialog id="my_modal_5" class="modal modal-open sm:modal-middle">
		<div class="modal-box">
			<h3 class="text-lg font-bold">Hello!</h3>
			<p class="py-4">Press ESC key or click the button below to close</p>
			<div class="modal-action">
				<button class="btn fixed top-4 right-4" hx-on:click='document.getElementById("my_modal_5").remove()'>X</button>
			</div>
		</div>
	</dialog>
}
